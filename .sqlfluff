[sqlfluff]
dialect = sparksql
templater = jinja
exclude_rules = L016, L019, L030, L036, L043, L003, L009

[sqlfluff:rules]
layout:type:comma:line_position = leading

[sqlfluff:rules:L001]
tab_space_size = 4
[sqlfluff:rules:L010]
capitalisation_policy = upper

[sqlfluff:rules:L014]
extended_capitalisation_policy = consistent
unquoted_identifiers_policy = aliases

